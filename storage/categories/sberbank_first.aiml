                    <pattern>*</pattern>
                    <that> ХОТИТЕ УЗНАТЬ О БОНУСАХ ПОПОДРОБНЕЕ </that>
                    <template><srai>MU001</srai> Хотите узнать о бонусах поподробнее?</template>
                </category>

