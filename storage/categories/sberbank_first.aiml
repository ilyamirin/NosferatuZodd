                    <pattern> ^ <regex pattern="ДА|.*СКАЖИ|ОК|ЛАДНО|ХОРОШО|ДАВАЙ|ХОЧ(У|ЕТСЯ)|ГОВОРИ|ИНТЕРЕС|ЛА.*|ОКЕЙ" /> ^ </pattern>
                    <that> ХОТИТЕ УЗНАТЬ О БОНУСАХ ПОПОДРОБНЕЕ </that>
                    <template><think><set name = "topic">sb_1_bonuses</set></think><srai>OK001</srai><srai>DIALOG1 BONUSES</srai></template>
                </category>
                <category>
                    <pattern> ^ <regex pattern="НЕ.*" /> ^ </pattern>
