                <pattern> ^ <regex pattern="ДА|.*СКАЖИ|ОК|ЛАДНО|ХОРОШО|ДАВАЙ|ХОЧ(У|ЕТСЯ)|ГОВОРИ|ИНТЕРЕС|ЛА.*|ОКЕЙ" /> ^ </pattern>
                <that> ОФОРМЛЯЕМ </that>
                <template><think><set name="topic">unknown</set></think><srai> ДА </srai></template>
